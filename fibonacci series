import java.util.*;
import java.lang.*;
import java.io.*;


class findarea
{
  static int cal(int n){
    if(n<=0) return 0;
    int f[]=new int[n+1];
    f[0]=0;
    f[1]=1;
    int s=f[0]+f[1];
    for(int i=2;i<=n;i++){
      f[i]=f[i-1]+f[i-2];
      s+=f[i];
    }
    return s;
    }
	public static void main (String[] args) throws java.lang.Exception
	{
	    int n=4;
	    System.out.println(cal(n));
	}
}
