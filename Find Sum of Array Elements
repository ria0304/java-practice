import java.util.*;
import java.lang.*;
import java.io.*;



class findarea
{
    static int a[]={12,3,4,15};
    static int s(){
      int  s=0;
      for(int i=0;i<a.length;i++){
        s+=a[i];
      }
      return s;
    }
	public static void main (String[] args) throws java.lang.Exception
	{

	    System.out.println("sum of given array is "+ s());
	    
	}
}
